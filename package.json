{
  "name": "fortune-sheet",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "packages/*"
  ],
  "repository": "https://github.com/ruilisi/fortune-sheet",
  "license": "MIT",
  "scripts": {
    "dev": "yarn storybook",
    "build": "father-build",
